{
    "linters": {
        "chmod": {
            "type": "chmod"
        },
        "filename": {
            "type": "filename"
        },
        "generated": {
            "type": "generated"
        },
        "eslint": {
            "type": "eslint",
            "include": "(\\.ts$)",
            "bin": "./node_modules/.bin/eslint"
        },
        "merge-conflict": {
            "type": "merge-conflict"
        },
        "nolint": {
            "type": "nolint"
        },
        "spelling": {
            "type": "spelling"
        },
        "text": {
            "type": "text",
            "text.max-line-length": 120,
            "severity": {
                "2": "disabled"
            }
        }
    },
    "exclude": [
        "/package-lock.json/"
    ]
}
